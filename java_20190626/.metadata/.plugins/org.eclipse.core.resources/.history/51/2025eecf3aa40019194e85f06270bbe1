package com.exam;

public class dajeong5 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		 
		int[][]num = new int[4][4];
		
		for(int i=0; i<num.length; i++) {
			for(int e=0; e<num[i].length; e++) {
				num[i][e] = (int)(Math.random()*16+1);
						 System.out.println(num[i][e] + "\t");
			}
			System.out.println();
		}
		
		
		
		
		
	}

}
