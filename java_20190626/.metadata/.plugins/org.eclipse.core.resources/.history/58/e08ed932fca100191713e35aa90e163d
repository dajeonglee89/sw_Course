package com.exam;

public class Ex3 {
	public static int hello(int a, int b) {
		int c = a + b;
		return c;
	}
		
	public static int abs(int a) {
		int b = a;
		if (a < 0) {
			b *= -1;
		}
		return b;
	
	}
	
	
	
	
	public static void main(String[] args) {

	System.out.println(hello(10, 20));
	abs(-5);
	
	System.out.println(abs(-5));
	
	
	
	
	
	
	
	
	}

}
