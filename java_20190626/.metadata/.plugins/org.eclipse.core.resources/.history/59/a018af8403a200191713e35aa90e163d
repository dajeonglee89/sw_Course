package com.exam;

public class Ex3 {
	public static int hello(int a, int b) {
		int c = a + b;
		return c;
	}
		
	public static int abs(int a) {
		//int b = a;
		if (a < 0) {
			a *= -1;
		}
		return a;
	}
	
	public static int myMax(int a, int b) {
		int m  = 0;
		if(a < b) {
			m = b;
		}else {
			m = a;
		}
		return m;
	}
	
	public static int mySum() {
		int s = 0;
		for(int i = 1; i<11; i++) {
			s += i;
		}
		return s;
	}
	
	
	public static void main(String[] args) {

	System.out.println(hello(10, 20));
	System.out.println("-5의 절대값 : " + abs(5));
	System.out.println("10과 20중 큰 수는 : " + myMax(10,20));
	System.out.println("1 ~ 10 까지의 합 : " +mySum());
	
	
	
	
	
	
	}

}
