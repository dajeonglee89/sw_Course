package com.exam;

class Tree{
	//필드
	String name;
	int age;
	void wave() {
		System.out.println(name + "가 바람에 흔들린다.");
	}
}









public class Ex4 {

	public static void main(String[] args) {

		int a = 0;
		int arr[] = new int[5];
		
		Tree tree;
		tree = new Tree();
		tree.name = "소나무";
		tree.age = 100;
		System.out.println(tree.name);
		System.out.println(tree.age);
		tree.wave();
		
		
		
		
		
		
	}

}
