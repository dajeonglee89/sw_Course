package com.exam;

public class Ex1 {
	public static void main(String[] args) {

	int scoArr[][] = {{85,60,70},{90,95,80},{75,80,100},{80,70,95},{100,65,80}};
		for(int i = 0; i<scoArr.length; i++) {
			for(int j = 0; j<scoArr[i].length; j++) {
				System.out.printf("%d번 학생 : %d\t",i+1,scoArr[i][j]);
			}
		System.out.println();
		}
	
	
	
	
	}

}
